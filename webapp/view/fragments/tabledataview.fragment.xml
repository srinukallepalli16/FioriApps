<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:u="sap.ui.unified">
  <Table id="table" items="{tableXLData>/result}">
    <columns >
      <Column id="column1" >
        <Label id="label1" text="{/header}"/>
      </Column>
      <Column id="column2">
        <Label id="label2" text="{/header}"/>
      </Column>
      <Column id="column3">
        <Label id="label3" text="{/header}"/>
      </Column>
      <Column id="column4">
        <Label id="label4" text="{/header}"/>
      </Column>
      <Column id="column5">
        <Label id="label5" text="{/header}" />
      </Column>
      <Column id="column6" >
        <Label id="label6" text="{/header}"/>
      </Column>
      <Column id="column7" >
        <Label id="label" text="{/header}"/>
      </Column>
    </columns> 
        <items>
            <ColumnListItem id="columnlistitem1">
                <cells>
                    <Text id="text1" text="{tableXLData>0}" />
                    <Text id="text2" text="{tableXLData>1}" />
                    <Text id="text3" text="{tableXLData>2}" />
                    <Text id="text4" text="{tableXLData>3}" />
                    <Text id="text5" text="{tableXLData>4}" />
                    <Text id="text6" text="{tableXLData>5}" />
                    <Text id="text7" text="{tableXLData>6}" />
                </cells>
            </ColumnListItem>
        </items>
  </Table>
</core:FragmentDefinition>


